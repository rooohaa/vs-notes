{
  "name": "vs-notes",
  "displayName": "VS-Notes",
  "description": "With this extension you can take notes in vscode easily.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vs-notes-sidebar",
    "onCommand:vs-notes.helloWorld",
    "onCommand:vs-notes.askQuestion",
    "onCommand:vs-notes.refresh"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vs-notes-sidebar-view",
          "title": "VSNotes",
          "icon": "media/notebook.svg"
        }
      ]
    },
    "views": {
      "vs-notes-sidebar-view": [
        {
          "type": "webview",
          "id": "vs-notes-sidebar",
          "name": "VSNotes",
          "icon": "media/notebook.svg",
          "contextualTitle": "VSNotes"
        }
      ]
    },
    "commands": [
      {
        "category": "VS-Notes",
        "title": "Hello World",
        "command": "vs-notes.helloWorld"
      },
      {
        "category": "VS-Notes",
        "title": "Ask Question",
        "command": "vs-notes.askQuestion"
      },
      {
        "category": "VS-Notes",
        "title": "Refresh",
        "command": "vs-notes.refresh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
